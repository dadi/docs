{
  "caching": {
    "ttl": 3600,
    "directory": {
      "enabled": false,
      "path": "./cache/"
    },
    "redis": {
      "enabled": false,
      "host": "127.0.0.1",
      "port": 6379
    }
  },
  "server": {
    "host": "0.0.0.0",
    "port": 3001
  },
  "security": {
    "trustProxy": true,
    "transportSecurity": false
  },
  "cluster": false,
  "virtualDirectories": [{
    "path": "sandbox",
    "index": "index.html"
  }],
  "rewrites": {
    "forceTrailingSlash": true,
    "path": "./workspace/routes/rewrites.txt"
  }
}
