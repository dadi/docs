{
  "datasource": {
    "key": "search",
    "source": {
      "type": "markdown",
      "path": "./docs",
      "renderHtml": true
    },
    "count": 1,
    "paginate": true,
    "filter": {
      "published": true
    },
    "requestParams": [
      {
        "param": "product",
        "field": "attributes.product",
        "target": "filter"
      },
      {
        "param": "version",
        "field": "attributes.version",
        "target": "filter"
      }
    ]
  }
}